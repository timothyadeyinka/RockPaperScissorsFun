{
  "name": "rock-paper-scissors",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile:sass": "sass --no-source-map sass/main.scss dist/styles.css",
    "compile:sass:prod": "sass --style=compressed sass/main.scss dist/styles.css",
    "watch:sass": "sass --watch sass/main.scss:dist/styles.css --no-source-map",
    "build": "npm run compile:sass",
    "build:prod": "npm run compile:sass:prod"
  },
  "dependencies": {
    "sass": "^1.97.3"
  }
}
